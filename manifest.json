{
  "name": "Usercentrics Assistant",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Assistant to help in checking Usercentrics Implementation data.",
  "homepage_url": "http://example.com",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Usercentrics Assistant",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "resources/img/uc-logo.png",
      "32": "resources/img/uc-logo.png",
      "48": "resources/img/uc-logo.png",
      "128": "resources/img/uc-logo.png"
    }
  },
  "icons": {
    "16": "resources/img/uc-logo.png",
    "32": "resources/img/uc-logo.png",
    "48": "resources/img/uc-logo.png",
    "128": "resources/img/uc-logo.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "storage",
    "activeTab",
    "declarativeContent"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "mac": "Command+Shift+U"
      },
      "description": "Opens popup"
    }
  }
}
